<h3>Employee List</h3>
<a class="btn btn-secondary" href="/employee"><i class="fa fa-plus">Create New</i></a>

 <table class="table table-striped"> 
     <thead>
         <th>FullName</th>
         <th>Email</th>
         <th>Mobile</th>
         <th>City</th>
         <th></th>
     </thead>

     <tbody>
         {{#each list}}
         <tr>
             <td>{{{this.fullName}}}</td>
             <td>{{this.email}}</td>
             <td>{{this.mobile}}</td>
             <td>{{this.city}}</td>
             <td>
                 <a> <i href="" class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                 <a>  <i href="" class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                
                
             </td>
         </tr>
         {{/each}}
     </tbody>
 </table>